{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "identifier": "io.github.kitsura-vpn",
  "bundle": {
    "targets": ["deb", "rpm"],
    "linux": {
      "deb": {
        "depends": ["openssl"],
        "desktopTemplate": "./packages/linux/kitsura-vpn.desktop",
        "provides": ["kitsura-vpn"],
        "conflicts": ["kitsura-vpn"],
        "replaces": ["kitsura-vpn"],
        "postInstallScript": "./packages/linux/post-install.sh",
        "preRemoveScript": "./packages/linux/pre-remove.sh"
      },
      "rpm": {
        "depends": ["openssl"],
        "desktopTemplate": "./packages/linux/kitsura-vpn.desktop",
        "provides": ["kitsura-vpn"],
        "conflicts": ["kitsura-vpn"],
        "obsoletes": ["kitsura-vpn"],
        "postInstallScript": "./packages/linux/post-install.sh",
        "preRemoveScript": "./packages/linux/pre-remove.sh"
      }
    },
    "externalBin": [
      "./resources/kitsura-service",
      "./resources/install-service",
      "./resources/uninstall-service",
      "./sidecar/kitsura-core",
      "./sidecar/kitsura-core-alpha"
    ]
  }
}
